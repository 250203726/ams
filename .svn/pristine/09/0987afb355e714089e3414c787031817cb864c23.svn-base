}

a {
	color: #666;
	font-size: 13px;
	text-decoration: none; 
	-webkit-transition: all .3s ease-out;
	-moz-transition: all .3s ease-out;
	-ms-transition: all .3s ease-out;
	-o-transition: all .3s ease-out;
	transition: all .3s ease-out;
}

a:hover {
	color: #333;
	text-decoration: underline;
}

img {
	border-width: 0px;
	display: inline;
}

ol,ul,dl {
	display:block;
	list-style: none;
}

*{
	margin: 0;
	padding: 0;
}

.clear{
	clear: both;
}

.hide{
	display: none;
}

.show{
	display:block;
}

tr.show{
	display: table-row;
}

td.show{
	display: table-cell;
}

