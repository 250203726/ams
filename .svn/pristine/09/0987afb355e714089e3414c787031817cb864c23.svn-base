	min-width: 80px;
	background: #FFF url(../images/icn_date.gif) no-repeat right center;
}

/*----------- operation content search section -----------*/

table caption{
	color: #006e6b;
	font-family: "楷体";
	font-size: 1.5em;
	font-weight: 700;
	padding: 10px 0;
}

table.op_tb{
	margin: 0 auto;
	width: 100%;
}

.op_tb input[type="text"],
.op_tb input[type="password"] 
.op_tb select {
	height: 24px;
	line-height: 24px;
}

.op_tb input[type="text"],
.op_tb input[type="password"] {
	width: 60%;
}
	
.op_tb th{
	height: 24px;
	white-space: nowrap;	
