package com.ams.system.entity;

/**
 * 字典表实体
 * @author Wymann
 * @Date 2013-11-18 下午04:13:11
 *
 */
public class Dictionary {

	private String id;				//系统编号
	private String groupname;		//组名
	private String groupTranslate;	//组名翻译
	private String code;			//值对应中文翻译
	private String name;			//值对应英文翻译
	private String value;			//值
	private Integer sortCode;		//排序
	private String filter;			//值分组,大于，等于，小于某个值来分组
	private String exesql;			//执行的sql
	private String remark;			//备注
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getGroupname() {
		return groupname;
	}
	public void setGroupname(String groupname) {
		this.groupname = groupname;
	}
	public String getGroupTranslate() {
		return groupTranslate;
	}
	public void setGroupTranslate(String groupTranslate) {
		this.groupTranslate = groupTranslate;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}

	public String getValue() {
		return value;
	}
	public void setValue(String value) {
		this.value = value;
	}
	/**
	 * @return the sortCode
	 */
	public Integer getSortCode() {
		return sortCode;
	}
	/**
	 * @param sortCode the sortCode to set
	 */
	public void setSortCode(Integer sortCode) {
		this.sortCode = sortCode;
	}
	public String getFilter() {
		return filter;
	}
	public void setFilter(String filter) {
		this.filter = filter;
	}
	public String getExesql() {
		return exesql;
	}
	public void setExesql(String exesql) {
		this.exesql = exesql;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}


}
